{
  "name": "sonata/jwt-auth",
  "description": "JWT auth module for Sonata framework",
  "type": "library",
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "sonata/framework": "^1.0",
    "firebase/php-jwt": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Sonata\\JwtAuth\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
